--
-- Selected TOC Entries:
--
DROP INDEX "Preise_name";
DROP TABLE "preise";
--
--
--

CREATE TABLE "preise" (
	"name" text NOT NULL,
	"kosten" double precision NOT NULL,
	"einheit" text NOT NULL,
	"art" text NOT NULL,
	Constraint "Preise_pkey" Primary Key ("name")
);

