--
-- Selected TOC Entries:
--
DROP INDEX "charaktere_fertigkeiten_charakt";
DROP TABLE "charaktere_fertigkeiten";
--
--
--

CREATE TABLE "charaktere_fertigkeiten" (
	"charakter_name" text,
	"version" text,
	"fertigkeit" text,
	"wert" integer,
	"art" text,
	"av_bonus" integer,
	"sl_bonus" integer,
	"region" text,
	"magisch" text,
	"zauberwerk_stufe" text
);

--
--
--

CREATE INDEX charaktere_fertigkeiten_charakt ON charaktere_fertigkeiten USING btree (charakter_name text_ops, "version" text_ops);

