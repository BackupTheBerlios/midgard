DROP INDEX "ruestung_ruestung_s";
DROP TABLE "ruestung";
--
--
--

CREATE TABLE "ruestung" (
	"ruestung" text,
	"ruestung_s" text,
	"lp_verlust" integer,
	"min_staerke" integer,
	"rw_verlust" integer,
	"b_verlust" integer,
	"region" text,
	"abwehr_bonus_verlust" integer,
	"angriffs_bonus_verlust" integer,
	"vollruestung" integer
);

