CREATE TABLE "zauber" (
	"name" text NOT NULL,
	"kosten" integer NOT NULL,
	"ap" text NOT NULL,
	"art" text NOT NULL,
	"stufe" text NOT NULL,
	"region" text,
	"element" text,
	"zauberart" text,
	"agens" text,
	"prozess" text,
	"reagens" text,
	"zauberdauer" text NOT NULL,
	"reichweite" text NOT NULL,
	"wirkungsziel" text NOT NULL,
	"wirkungsbereich" text NOT NULL,
	"wirkungsdauer" text NOT NULL,
	"ursprung" text,
	"material" text,
	"spruchrolle" boolean DEFAULT 'f'::bool NOT NULL,
	"thaumagram" text
);

