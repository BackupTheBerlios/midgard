DROP INDEX "spezialgebiete_spezialgebiet";
DROP TABLE "spezialgebiete";
--
--
--

CREATE TABLE "spezialgebiete" (
	"typ" text NOT NULL,
	"nr" integer NOT NULL,
	"spezialgebiet" text,
	"spezial" text,
	"spezial2" text,
	Constraint "spezialgebiete_pkey" Primary Key ("typ", "nr")
);

