DROP INDEX "schrift_name";
DROP TABLE "schrift";
--
--
--

CREATE TABLE "schrift" (
	"name" text,
	"typ" text,
	"kosten" integer,
	"region" text,
	"alt" boolean,
	"kult" boolean
);

