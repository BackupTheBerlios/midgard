--
-- Selected TOC Entries:
--
DROP INDEX "sprachen_name";
DROP TABLE "sprachen";
--
--
--

CREATE TABLE "sprachen" (
	"fp" integer,
	"name" text,
	"max_wert" integer,
	"schrift" text,
	"alt" boolean,
	"region" text,
	"gruppe_1" integer,
	"gruppe_2" integer,
	"minderheit" boolean
);

