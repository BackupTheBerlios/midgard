--
--

CREATE TABLE "typen" (
	"typnr" integer,
	"typs" text,
	"typl" text,
	"typlw" text,
	"typz" text,
	"ausdauer" text,
	"stand" integer,
	"sb" integer,
	"ruestung" integer,
	"region" text,
	"geld" integer,
	"beruf" text,
	"stadt_land" text,
	"sprueche_mit_pp" text,
	"nsc_only" boolean,
	"st" integer,
	"gw" integer,
	"gs" integer,
	"in" integer,
	"pa" integer,
	"kultwaffe" boolean,
	"lernpflichten" text
);

