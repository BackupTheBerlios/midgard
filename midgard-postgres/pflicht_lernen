DROP INDEX "pflicht_lernen_typ";
DROP TABLE "pflicht_lernen";
--
--
--

CREATE TABLE "pflicht_lernen" (
	"typ" text,
	"pflicht" text,
	"lernpunkte" integer,
	"verboten" text,
	"spielbegin" text,
	"erfolgswert" integer
);

