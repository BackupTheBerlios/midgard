CREATE TABLE ruestung (
	ruestung text,
	ruestung_s text,
	lp_verlust integer,
	min_staerke integer,
	rw_verlust integer,
	b_verlust integer,
	region text
);

--
-- Data for TOC Entry ID 3 (OID 326300) TABLE DATA ruestung
