--
-- Selected TOC Entries:
--
DROP TABLE "regionen";
--
--
--

CREATE TABLE "regionen" (
	"nr" integer NOT NULL,
	"name" text NOT NULL,
	"abkuerzung" text NOT NULL,
	"file" text,
	"url" text,
	"maintainer" text NOT NULL,
	"version" text NOT NULL,
	"copyright" text,
	"offiziell" boolean DEFAULT 'f'::bool,
	"pic" integer,
	Constraint "regionen_pkey" Primary Key ("name")
);

