CREATE TABLE regionen (
    nr integer NOT NULL,
    name text NOT NULL,
    titel text NOT NULL,
    abkuerzung text NOT NULL,
    file text,
    url text,
    maintainer text NOT NULL,
    "version" text NOT NULL,
    copyright text,
    offiziell boolean DEFAULT false,
    pic integer,
    jahr text
);


