DROP TABLE "preise_modifikation";
--
--
--

CREATE TABLE "preise_modifikation" (
	"name" text NOT NULL,
	"faktor" double precision NOT NULL,
	"min" integer,
	"einheit" text,
	"art" text NOT NULL,
	"typ" text,
	"nr" integer,
	Constraint "preise_modifikation_pkey" Primary Key ("name")
);

